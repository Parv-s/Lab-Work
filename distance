#include <stdio.h>
#include <math.h>
int main() {
    float x1, y1, x2, y2;
    float distance;
    printf("Enter point 1 coordinates: ");
    scanf("%f %f", &x1, &y1);
    printf("Enter point 2 coordinates: ");
    scanf("%f %f", &x2, &y2);
    distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
    printf("\nEuclidean distance: %.2f\n", distance);
    return 0;
}
